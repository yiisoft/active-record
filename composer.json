{
    "name": "yiisoft/active-record",
    "type": "library",
    "description": "Yii ActiveRecord Library",
    "keywords": [
        "yii",
        "active record"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/active-record",
        "issues": "https://github.com/yiisoft/active-record/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/db": "^3.0@dev",
        "yiisoft/strings": "^3.0@dev"

    },
    "require-dev": {
        "hiqdev/composer-config-plugin": "^1.0@dev",
        "phpunit/phpunit": "^8.4",
        "yiisoft/db-mysql": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/event-dispatcher": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\ActiveRecord\\": "src",
            "Yiisoft\\ActiveRecord\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "tests": "config/tests.php"
        }
    },
    "config": {
        "sort-packages": true
    }
}
